{
  "name": "opuscodium-wormhole",
  "version": "2.2.1-rc0",
  "source": "https://github.com/opus-codium/puppet-wormhole",
  "author": "Opus Codium",
  "license": "Apache-2.0",
  "summary": "The wormhole module lets you use Puppet / Bolt / Choria to manage magic wormhole and transfer files between nodes",
  "project_page": "https://github.com/opus-codium/puppet-wormhole",
  "issues_url": "https://github.com/opus-codium/puppet-wormhole/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.6.0 < 10.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "10",
        "11"
      ]
    },
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "11",
        "12",
        "13"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.0.0 < 8.0.0"
    }
  ]
}
